# Server-side Sentinel Hub credentials (never commit secrets)
SENTINEL_HUB_CLIENT_ID=your_client_id_here
SENTINEL_HUB_CLIENT_SECRET=your_client_secret_here
# Optional: you can also provide a pre-requested token instead of client credentials
# SENTINEL_HUB_ACCESS_TOKEN=your_bearer_token_here
# Processing endpoint (default sentinel services URL)
# SENTINEL_HUB_PROCESSING_URL=https://services.sentinel-hub.com/api/v1/process
# Worker port
# WORKER_PORT=4000
OPENWEATHER_API_KEY=your_actual_key_here
